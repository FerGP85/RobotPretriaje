        "z": "97d8e83a441af8aa",
        "name": "Leer por ID",
        "func": "const id  = msg.req.params.id;\nconst key = 'pretriage_' + id;\n\nconst data = flow.get(key);\n\nif (!data) {\n  msg.statusCode = 404;\n  msg.payload = { error: 'ID no encontrado' };\n} else {\n  msg.statusCode = 200;\n  msg.payload = data;\n}\n\nreturn msg;",
