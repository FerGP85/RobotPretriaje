    {
        "id": "4ec373c45f14dd53",
        "type": "function",
        "z": "97d8e83a441af8aa",
        "name": "Parse/validar JSON",
        "func": "// Espera JSON del kiosk. Asegura objeto y normaliza.\nlet p = msg.payload;\nif (Buffer.isBuffer(p)) p = p.toString();\nif (typeof p === 'string') {\n  try { p = JSON.parse(p); } catch(e){ node.error('JSON inválido de WS', msg); return null; }\n}\nif (!p || typeof p !== 'object') { node.error('Payload no es objeto', msg); return null; }\n// Completa label si falta\nfunction sevLabel(s){\n  if(s==='high') return 'Crítico alto';\n  if(s==='mid')  return 'Crítico medio';\n  if(s==='low')  return 'Crítico bajo';\n  return 'No crítico';\n}\nif (p.severity && !p.severity_label) p.severity_label = sevLabel(p.severity);\nmsg.payload = p;\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 380,
        "y": 140,
        "wires": [
            [
                "15f78377df6cfd1c",
                "e1ca2b8c36a46820",
                "e874205c9d75aa6a"
            ]
        ]
    },
    {
        "id": "e874205c9d75aa6a",
        "type": "function",
        "z": "97d8e83a441af8aa",
        "name": "Guardar por ID",
        "func": "const p = msg.payload;\n\nif (!p || typeof p !== 'object') {\n  node.warn('Payload no es objeto, no se guarda');\n  return msg;\n}\n\nconst id = p.share_id;\nif (!id) {\n  node.warn('Mensaje sin share_id, no se guarda');\n  return msg;\n}\n\nconst key = 'pretriage_' + id;\nflow.set(key, p);\n\nnode.status({ fill: 'green', shape: 'dot', text: id });\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 690,
        "y": 80,
        "wires": [
            []
